<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pgrrr.onedelivery.mapper.StoreMapper">
    <select id="selectStoreListByCategory" parameterType="long" resultType="Store">
        SELECT *
        FROM store
        WHERE category_id = #{categoryId};
    </select>
    <select id="selectStoreListByCategoryAddress" parameterType="long" resultType="Store">
        SELECT *
        FROM store
        WHERE category_id = #{categoryId}
        AND address = #{address};
    </select>

</mapper>